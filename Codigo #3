def longitud(elemento):#Inicia la funcion para contador
    contador = 0
    for _ in elemento:#Se crea el bucle para contador
        contador += 1
    return contador
mi_lista = [10, 20, 25, 30, 40] #lista de num
print(longitud(mi_lista)) 
