def es_vocal(caracter):
    return caracter.lower() in 'aeiou'

#Nos muestra las vocales 
print(es_vocal('a'))  
print(es_vocal('e')) 
print(es_vocal('i'))  
print(es_vocal('o'))  
print(es_vocal('u'))  
